{
  "name": "polyv-rum-sdk",
  "version": "0.1.13",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --watch --format cjs,esm --dts --sourcemap",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "test": "jest",
    "prepare": "npm run build",
    "release:patch": "npm test && npm run build && npm version patch && git push && git push --follow-tags"
  },
  "peerDependencies": {
    "vue": ">=2.6.0 || >=3.2.0"
  },
  "dependencies": {
    "@mitojs/browser": "^3.0.1",
    "@aliyun-sls/web-track-browser": "^0.3.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "tsup": "^7.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "@types/jest": "^29.5.5"
  }
}
